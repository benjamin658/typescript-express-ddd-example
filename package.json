{
  "name": "typescript-express-ddd-example",
  "version": "0.1.0",
  "description": "Domain Driven Design is an approach to software design. This repository is an example of using Typescript and Express framework to build a RESTful API server in DDD way.",
  "main": "index.js",
  "scripts": {
    "up": "docker-compose -f ./deploy/docker/prod/docker-compose.yml -p typescript-express-ddd-example up --build -d",
    "down": "docker-compose -f ./deploy/docker/prod/docker-compose.yml -p typescript-express-ddd-example down -v",
    "up:dev": "docker-compose -f ./deploy/docker/dev/docker-compose.yml -p typescript-express-ddd-example up --build",
    "down:dev": "docker-compose -f ./deploy/docker/dev/docker-compose.yml -p typescript-express-ddd-example down -v",
    "build": "rimraf dist && babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "type-check": "tsc"
  },
  "author": "Ben Hu <benjamin658gae@gmail.com>",
  "license": "WTFPL",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/helmet": "0.0.43",
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.6.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-babel-module": "5.0.1",
    "eslint-plugin-import": "^2.18.0",
    "rimraf": "^2.6.3",
    "sequelize-cli": "^5.5.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "http-status": "^1.3.2",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.9.4",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  }
}
